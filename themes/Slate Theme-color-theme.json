{
	"name": "Slate Theme",
	"colors": {
		"editor.background": "#2a2b2d",
		"editor.foreground": "#eeffff",
		"activityBarBadge.background": "#007acc",
		"sideBarTitle.foreground": "#bbbbbb",
		"sideBar.background": "#323337",
		"tab.activeBackground": "#323337",
		"editorBracketHighlight.foreground1": "#bb729b",
		"editorBracketHighlight.foreground2": "#8CAA64",
		"editorBracketHighlight.foreground3": "#78d3ab",
		"editorBracketHighlight.foreground4": "#6eb1de",
		"editorBracketHighlight.foreground5": "#737bf0",
		"editorBracketHighlight.foreground6": "#a16bfc",
		"editorBracketHighlight.unexpectedBracket.foreground": "#eec890",
	},
	"tokenColors": [
		{
			"scope": "comment",
			"settings": {
				"foreground": "#64707e",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "keyword.control.import, keyword.control.export, keyword.control.from,  keyword.control.flow, support.type.primitive",
			"settings": {
				"foreground": "#bb729b",
			}
		},
		{
			"scope": "entity.name.type",
			"settings": {
				"foreground": "#6eb1de",
			}
		},
		{
			"scope": "storage.type.tsx, storage.type.type",
			"settings": {
				"foreground": "#9d6ec2",
			}
		},
		{
			"scope": "meta.var.expr, keyword.operator.assignment, variable.other.property, storage.type.function.arrow, storage.type, keyword.operator.optional, keyword.operator.type.annotation",
			"settings": {
				"foreground": "#6eb1de",
			}
		},
		{
			"scope": "string.quoted.double",
			"settings": {
				"foreground": "#8CAA64",
			}
		},
		{
			"scope": "entity.other.attribute-name, punctuation.definition.bracket.square",
			"settings": {
				"foreground": "#78d3ab",
			}
		},
		{
			"scope": "keyword.operator.type.annotation, meta.brace.round.tsx, punctuation.definition.block, punctuation.definition.typeparameters.begin, meta.objectliteral,  meta.tag.attributes, punctuation.definition.tag.end, punctuation.definition.tag.begin, meta.import, meta.var-single-variable.expr, keyword.operator.assignment",
			"settings": {
				"foreground": "#f8f8f8",
			}
		},
		{
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#737bf0",
			}
		},
		{
			"scope": "meta.function-call",
			"settings": {
				"foreground": "#e669f9",
			}
		},
	],
}
